<script type="text/javascript">
			
	window.onload = function() {

		var ajax_path = "https://web.../web/20181117055336/https://www."+".."+"/bin/splunk/geoipservice";
        
		$.ajax({
			cache: false,
			type: "GET",
			url: ajax_path,
			dataType: "json",
			async: false,
			statusCode: {
				500: function() {
					if (typeof(Storage) !== "undefined") {
						sessionStorage.mkt_region = "AMER";
					}
				}
			},
			success: function(response) {
				var region = response.region;
				if (!region) {
					region = "AMER";
				}
				console.log("Region in: " + region);
				if (typeof(Storage) !== "undefined") {
					sessionStorage.mkt_region = region;
				}
			},
			error: function(error) {
				if (typeof(Storage) !== "undefined") {
					sessionStorage.mkt_region = "AMER";
				}
			}
		});

		function getCookie(name) {
            var re = new RegExp(name + "=([^;]+)");
            var value = re.exec(document.cookie);
            return (value != null) ? unescape(value[1]) : null;
        }
		console.log("Cookie scb_sk: " + getCookie('scb_sk'));
		if(getCookie('scb_sk') == null && sessionStorage.getItem('mkt_region') == 'EMEA'){   
	        if($(".cookie-banner-centered").hasClass("hide")){
	            $(".cookie-banner-centered").removeClass("hide")
	        }
	    } else{
	        if(!$(".cookie-banner-centered").hasClass("hide")){
	            $(".cookie-banner-centered").addClass("hide")
	        }
	    }
	    $(".close-x-cookie-banner, .cookie-dismiss-cookie-banner").click(function(){
	        if(!$(".cookie-banner-centered").hasClass("hide")){
				var checkintkt = Math.random().toString(36).substring(2, 15) + Math.random().toString(36).substring(2, 15);
				document.cookie ="scb_sk="+checkintkt+"; expires=375; domain=...; path=/";
	           $(".cookie-banner-centered").addClass("hide")
            }
	    })
    };

</script>
